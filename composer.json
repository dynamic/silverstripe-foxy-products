{
    "name": "dynamic/silverstripe-foxy-products",
    "description": "Use silverstripe-products for product page types using silverstripe-foxy",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "require": {
        "dynamic/silverstripe-foxy": "^1.0",
        "dynamic/silverstripe-products": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "theseer/phpdox": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Foxy\\Products\\": "src/",
            "Dynamic\\Foxy\\Products\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:dynamic/silverstripe-products.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
